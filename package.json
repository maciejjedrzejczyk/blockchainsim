{
  "name": "blockchainsim",
  "version": "1.3.0",
  "description": "Blockchain Demonstration Tool - An advanced, educational blockchain demonstration designed for non-technical audiences",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.4"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "*.js",
      "!node_modules/**"
    ],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  },
  "keywords": [
    "blockchain",
    "demonstration",
    "educational",
    "cryptocurrency",
    "mining",
    "role-based",
    "security",
    "digital-signatures",
    "proof-of-work",
    "simulator"
  ],
  "author": "blockchainsim contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/blockchainsim.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/blockchainsim/issues"
  },
  "homepage": "https://github.com/yourusername/blockchainsim#readme"
}
